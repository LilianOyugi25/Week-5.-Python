class Animal:
    def move(self):
        pass

class Lion(Animal):
    def move(self):
        return "Simba prowls silently through the grass! 游부"

class Wildebeest(Animal):
    def move(self):
        return "Wildebeest migrates in large herds! 游붧"

class Eagle(Animal):
    def move(self):
        return "Tai soars above the Mara! 游분"

# Usage
animals = [Lion(), Wildebeest(), Eagle()]
for animal in animals:
    print(animal.move())

# Output:
# Simba prowls silently through the grass! 游부
# Wildebeest migrates in large herds! 游붧
# Tai soars above the Mara! 游분
